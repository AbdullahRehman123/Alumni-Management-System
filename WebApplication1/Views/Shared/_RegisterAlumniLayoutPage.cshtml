                $.get("/members/GetCategoryPrograms", { category: $("#category_studied").val(), campus: $("#campus").val() }, function (data) {
                    $("#program_studied").empty();
                    $.each(data, function (index, row) {
                        if (index == 1) {
                            $("#program_studied").append("<option selected='selected' value='" + row + "'>" + row + "</option>")
                        }
                        else if (index == 0) {
                            $("#program_studied").append("<option disabled='disabled' value='" + row + "'>" + row + "</option>")
                        }
                        else {
                            $("#program_studied").append("<option  value='" + row + "'>" + row + "</option>")

                        }
                    });
                });
            });

            $("#category_studied").change(function () {
                $.get("/members/GetCategoryPrograms", { category: $("#category_studied").val(), campus: $("#campus").val() }, function (data) {
                    $("#program_studied").empty();
