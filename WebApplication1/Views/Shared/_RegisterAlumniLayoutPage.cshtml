        <script type="text/javascript">




            $(document).ready(function () {

                $("#alumni-login-button").click(function (e) {
                    e.preventDefault();
                    //Serialize the form datas.
                    var valdata = $("#alumni-login-form").serialize();
                    //to get alert popup

                    $.ajax({
                        url: "/members/LoginNew",
                        type: "POST",
                        dataType: 'json',
                        contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                        data: valdata,
                        success: function (response) {
                            if (response.redirectToUrl != undefined) {
                                window.location.href = response.redirectToUrl;
                            }
                            else {

                                $("#invalidCredentialsAlert").text(response.responseText);
                                $("#invalidCredentialsAlert").fadeTo(2000, 2000).fadeOut(2000, function () {
                                    $("#invalidCredentialsAlert").fadeOut(2000);
                                });
                            }
                        },
                        error: function (response) {
                            alert("Error");
                        }
                    });
                });
            });




        </script>

