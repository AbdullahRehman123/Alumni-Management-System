                                    @Html.ValidationMessageFor(model => model.email, "", new { @class = "valid-feedback text-danger" })
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group mx-5">
                                    <div class="input-group">

                                        @Html.EditorFor(model => model.password, new { htmlAttributes = new { @placeholder = "Password", @class = "form-control", required = "required", @maxlength = "50", @id = "password", @name = "password", @type = "password" } })
                                    </div>
                                    @Html.ValidationMessageFor(model => model.password, "", new { @class = "text-danger" })
                                </div>
