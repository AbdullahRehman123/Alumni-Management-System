            function isProgramValid(value) {
                if (value == "Choose Program") {
                    return false;
                }
                else {
                    return true;
                }
            }

            function isValid(value) {
                var fieldNum = /^[a-zA-Z ]*$/;

                if ((value.match(fieldNum))) {
                    return true
                }
                else {
                    return false
                }

            }

            $('#name').keypress(function (e) {
                var regex = new RegExp("^[a-zA-Z ]*$");
                var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
                if (regex.test(str)) {
                    return true;
                }
                else {
                    e.preventDefault();

                    return false;
                }
            });


