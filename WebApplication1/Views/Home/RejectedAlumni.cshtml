@using WebApplication1.Models
@{
    ViewBag.Title = "RejectedAlumni";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}

<style>
    * {
        -webkit-transition: none !important;
        -moz-transition: none !important;
        -o-transition: none !important;
        transition: none !important;
    }
</style>

<main class="m-5">
    @if (Model.rejectedMembersCount > 0)
    {
        <div class="flex-contianer justify-content-center">
            <div class="left-btn">
              
            </div>
        </div>
        <div class="small table-responsive">
            <table class="table table-sm table-striped table-danger" id="rejected-alumni-table">
                <thead>
                    <tr>
                        <th>
                            Name
                        </th>

                        <th>
                            Email
                        </th>
                        <th>
                            Mobile No
                        </th>
                        <th>
                            Reg No
                        </th>
                        <th>
                            Campus
                        </th>

                        <th>
                            Program Studied
                        </th>

                        <th>
                            Org/Comp Working In
                        </th>

                        <th>
                            Designation
                        </th>

                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                @foreach (member item in Model.rejected_members_list)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.name)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.mobile_no)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.registration_no)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.campus)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.program_studied)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.org_comp_working_in)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.designation)
                        </td>


                        <td>
                            <div class="btn-group" role="group" aria-label="Basic example">

                                <a class="px-1" href="@Url.Action("edit", "home",new { id = item.member_id } )" data-toggle="tooltip" data-placement="top" title="Edit Alumni" >

                                    <svg xmlns="http://www.w3.org/2000/svg" height="1.3em" viewBox="0 0 576 512"><path d="M402.6 83.2l90.2 90.2c3.8 3.8 3.8 10 0 13.8L274.4 405.6l-92.8 10.3c-12.4 1.4-22.9-9.1-21.5-21.5l10.3-92.8L388.8 83.2c3.8-3.8 10-3.8 13.8 0zm162-22.9l-48.8-48.8c-15.2-15.2-39.9-15.2-55.2 0l-35.4 35.4c-3.8 3.8-3.8 10 0 13.8l90.2 90.2c3.8 3.8 10 3.8 13.8 0l35.4-35.4c15.2-15.3 15.2-40 0-55.2zM384 346.2V448H64V128h229.8c3.2 0 6.2-1.3 8.5-3.5l40-40c7.6-7.6 2.2-20.5-8.5-20.5H48C21.5 64 0 85.5 0 112v352c0 26.5 21.5 48 48 48h352c26.5 0 48-21.5 48-48V306.2c0-10.7-12.9-16-20.5-8.5l-40 40c-2.2 2.3-3.5 5.3-3.5 8.5z" /></svg>

                                </a>
                            </div>
                        </td>
                    </tr>
                }
                    </tbody>
            </table>
        </div>

    }
</main>
<script src="~/Content/util.js"></script>
<script src="~/Content/menu-aim.js"></script>
<script src="~/Content/main.js"></script>